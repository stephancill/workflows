{
  "name": "img2img-nft",
  "version": "1.0.0",
  "inputs": [
    {
      "name": "CHAIN_ID",
      "type": "int"
    },
    {
      "name": "ADDRESS",
      "type": "string"
    },
    {
      "name": "TOKEN_ID",
      "type": "int"
    },
    {
      "name": "INPUT_PROMPT",
      "type": "string"
    }
  ],
  "outputs": [{ "name": "OUTPUT_IMAGE", "type": "string" }],
  "packages": [
    {
      "name": "std:nft-resolver",
      "commands": [
        "export INITIAL_IMAGE=$(node nft-resolver.js --chainId=$CHAIN_ID --tokenAddress=$ADDRESS --tokenId=$TOKEN_ID)"
      ],
      "exports": ["INITIAL_IMAGE"]
    },
    {
      "name": "std:img2img",
      "commands": [
        "export OUTPUT_IMAGE=\"data:image/png;base64,$(cat $(img2img --image=\"$INITIAL_IMAGE\" --prompt=\"$INPUT_PROMPT\") | base64)\""
      ],
      "exports": ["OUTPUT_IMAGE"]
    }
  ]
}
