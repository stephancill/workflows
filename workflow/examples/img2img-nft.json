{
  "name": "img2img-nft",
  "version": "1.0.0",
  "inputs": [
    {
      "name": "CHAIN_ID"
    },
    {
      "name": "ADDRESS"
    },
    {
      "name": "TOKEN_ID"
    },
    {
      "name": "INPUT_PROMPT"
    },
    {
      "name": "INPUT_SEED"
    }
  ],
  "outputs": [{ "name": "OUTPUT_IMAGE_FILE", "type": "string" }],
  "steps": [
    {
      "name": "std:nft-resolver",
      "commands": [
        "export INITIAL_IMAGE_FILE=\"$(node nft-resolver.js --chainId=$CHAIN_ID --tokenAddress=$ADDRESS --tokenId=$TOKEN_ID)\""
      ],
      "exports": ["INITIAL_IMAGE_FILE"]
    },
    {
      "name": "std:img2img",
      "commands": [
        "export OUTPUT_IMAGE_FILE=\"$(img2img --image-file=\"$INITIAL_IMAGE_FILE\" --prompt=\"$INPUT_PROMPT\" --seed=$INPUT_SEED)\""
      ],
      "exports": ["OUTPUT_IMAGE_FILE"]
    }
  ]
}
